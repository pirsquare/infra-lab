# Terraform Variables Example
# Copy this to terraform.tfvars and fill in your values

app_name     = "fastapi-app"
environment  = "dev"
target       = "kubernetes"
docker_image = "ghcr.io/YOUR_ORG/YOUR_REPO:latest"
container_port = 8000
replicas     = 1

tags = {
  project     = "fastapi-cloud"
  managed_by  = "terraform"
  environment = "dev"
}
